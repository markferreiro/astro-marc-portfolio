---
import Main from "../layouts/Main.astro";
import Header from "../components/Header.astro";
const { title } = Astro.props;
---

<Main title={title}>
  <main
    class="px-5 flex min-h-screen flex-col items-center justify-start pt-10 bg-gradient-to-br from-gradient2From to-gradient2To"
  >
    <Header />
    <slot />
  </main>
</Main>
